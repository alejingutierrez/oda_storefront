# Core
NEXTAUTH_SECRET=changeme
ADMIN_TOKEN=changeme-admin-token
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=change-me
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-5.2
OPENAI_WEB_SEARCH=true
VERCEL_TEAM_ID=your-vercel-team-id
VERCEL_TOKEN=your-vercel-token

# Database (Neon / Postgres)
DATABASE_URL=postgresql://user:password@pooler-host/dbname?sslmode=require
DATABASE_URL_UNPOOLED=postgresql://user:password@direct-host/dbname?sslmode=require
NEON_DATABASE_URL=${DATABASE_URL}
PGHOST=pooler-host
PGHOST_UNPOOLED=direct-host
PGUSER=user
PGDATABASE=dbname
PGPASSWORD=password
POSTGRES_URL=${DATABASE_URL}
POSTGRES_URL_NON_POOLING=${DATABASE_URL_UNPOOLED}
POSTGRES_URL_NO_SSL=postgresql://user:password@pooler-host/dbname
POSTGRES_PRISMA_URL=${DATABASE_URL}
POSTGRES_HOST=${PGHOST}
POSTGRES_USER=${PGUSER}
POSTGRES_PASSWORD=${PGPASSWORD}
POSTGRES_DATABASE=${PGDATABASE}

# Redis / queues
REDIS_URL=redis://redis:6379

# Storage (Vercel Blob)
VERCEL_BLOB_READ_WRITE_TOKEN=your-vercel-blob-token
BLOB_READ_WRITE_TOKEN=${VERCEL_BLOB_READ_WRITE_TOKEN}

# Billing
WOMPI_PUBLIC_KEY=your-wompi-public-key
WOMPI_PRIVATE_KEY=your-wompi-private-key

# Email
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=user
SMTP_PASS=password

# Scraper
USER_AGENT=ODA-Storefront-Scraper/0.1
BRANDS_XLSX_PATH=Marcas colombianas.xlsx
BRAND_SCRAPE_MAX_JOBS=3
